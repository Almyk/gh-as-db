{
  "name": "gh-as-db",
  "version": "1.1.1",
  "description": "Use a private GitHub repository as a database for your application.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "gh-as-db": "./dist/ui/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "github",
    "database",
    "storage",
    "orm",
    "json",
    "collection"
  ],
  "author": "Tommy Hansen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almyk/gh-as-db.git"
  },
  "bugs": {
    "url": "https://github.com/almyk/gh-as-db/issues"
  },
  "homepage": "https://github.com/almyk/gh-as-db#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@vitest/coverage-v8": "^4.0.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "enquirer": "^2.4.1"
  }
}
